{
    "compilerOptions": {
      "target": "es2020",
      "moduleResolution": "Node",
      "lib": [
        "es2020",
        "dom"
      ],
      "types": [
        "cypress",
        "node"
      ],
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts", "**/*.d.ts"]
  }